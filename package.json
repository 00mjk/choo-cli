{
  "name": "choo-cli",
  "version": "0.4.0",
  "description": "create choo apps from the command line",
  "bin": {
    "choo": "bin/cli.js",
    "choo-new": "bin/choo-new.js",
    "choo-generate": "bin/choo-generate.js"
  },
  "dependencies": {
    "cross-spawn": "^4.0.0",
    "inquirer": "^1.1.2",
    "lodash": "^4.14.1",
    "mem-fs": "^1.1.3",
    "mem-fs-editor": "^2.3.0",
    "minimist": "^1.2.0",
    "ramda": "^0.21.0",
    "resolve": "^1.1.7",
    "walk-back": "^2.0.1",
    "yamljs": "^0.2.8"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "scripts": {
    "clean": "rimraf temp",
    "pretest": "npm run clean",
    "posttest": "npm run clean",
    "test": "standard | snazzy && NODE_ENV=test tape --require ./tests/**/*.js | tap-summary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trainyard/choo-cli.git"
  },
  "keywords": [
    "choo.js",
    "generator",
    "scaffold",
    "plop",
    "yeoman-generator",
    "slush",
    "cli",
    "choo",
    "boilerplate",
    "template"
  ],
  "author": "Matt McFarland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trainyard/choo-cli/issues"
  },
  "homepage": "https://github.com/trainyard/choo-cli#readme",
  "devDependencies": {
    "clinton": "^0.7.1",
    "file-exists": "^2.0.0",
    "rimraf": "^2.5.4",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.0"
  },
  "standard": {
    "ignore": [
      "template"
    ]
  }
}
